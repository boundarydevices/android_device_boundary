PREBOOT_PART_SIZE=16
DTBO_PART_SIZE=4
BOOT_PART_SIZE=48
RECOVERY_PART_SIZE=48
SYSTEM_PART_SIZE=2816
CACHE_PART_SIZE=1024
MISC_PART_SIZE=4
METADATA_PART_SIZE=2
FRP_PART_SIZE=1
VENDOR_PART_SIZE=768
FBMISC_PART_SIZE=1
VBMETA_PART_SIZE=1

PREBOOT_PART_BEGIN=1
PREBOOT_PART_END=$(($PREBOOT_PART_BEGIN + $PREBOOT_PART_SIZE))
DTBO_PART_BEGIN=$PREBOOT_PART_END
DTBO_PART_END=$(($DTBO_PART_BEGIN + $DTBO_PART_SIZE))
BOOT_PART_BEGIN=$DTBO_PART_END
BOOT_PART_END=$(($BOOT_PART_BEGIN + $BOOT_PART_SIZE))
RECOVERY_PART_BEGIN=$BOOT_PART_END
RECOVERY_PART_END=$(($RECOVERY_PART_BEGIN + $RECOVERY_PART_SIZE))
SYSTEM_PART_BEGIN=$RECOVERY_PART_END
SYSTEM_PART_END=$(($SYSTEM_PART_BEGIN + $SYSTEM_PART_SIZE))
CACHE_PART_BEGIN=$SYSTEM_PART_END
CACHE_PART_END=$(($CACHE_PART_BEGIN + $CACHE_PART_SIZE))
MISC_PART_BEGIN=$CACHE_PART_END
MISC_PART_END=$(($MISC_PART_BEGIN + $MISC_PART_SIZE))
METADATA_PART_BEGIN=$MISC_PART_END
METADATA_PART_END=$(($METADATA_PART_BEGIN + $METADATA_PART_SIZE))
FRP_PART_BEGIN=$METADATA_PART_END
FRP_PART_END=$(($FRP_PART_BEGIN + $FRP_PART_SIZE))
VENDOR_PART_BEGIN=$FRP_PART_END
VENDOR_PART_END=$(($VENDOR_PART_BEGIN + $VENDOR_PART_SIZE))
FBMISC_PART_BEGIN=$VENDOR_PART_END
FBMISC_PART_END=$(($FBMISC_PART_BEGIN + $FBMISC_PART_SIZE))
VBMETA_PART_BEGIN=$FBMISC_PART_END
VBMETA_PART_END=$(($VBMETA_PART_BEGIN + $VBMETA_PART_SIZE))
DATA_PART_BEGIN=$VBMETA_PART_END
DATA_PART_END="100%"

export MKPART_COMMAND="\
mkpart preboot $PREBOOT_PART_BEGIN $PREBOOT_PART_END \
mkpart dtbo $DTBO_PART_BEGIN $DTBO_PART_END \
mkpart boot $BOOT_PART_BEGIN $BOOT_PART_END \
mkpart recovery $RECOVERY_PART_BEGIN $RECOVERY_PART_END \
mkpart system $SYSTEM_PART_BEGIN $SYSTEM_PART_END \
mkpart cache $CACHE_PART_BEGIN $CACHE_PART_END \
mkpart misc $MISC_PART_BEGIN $MISC_PART_END \
mkpart metadata $METADATA_PART_BEGIN $METADATA_PART_END \
mkpart frp $FRP_PART_BEGIN $FRP_PART_END \
mkpart vendor $VENDOR_PART_BEGIN $VENDOR_PART_END \
mkpart fbmisc $FBMISC_PART_BEGIN $FBMISC_PART_END \
mkpart vbmeta $VBMETA_PART_BEGIN $VBMETA_PART_END \
mkpart userdata $DATA_PART_BEGIN $DATA_PART_END \
"
